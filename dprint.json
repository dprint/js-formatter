{
  "incremental": true,
  "typescript": {
    "indentWidth": 2
  },
  "json": {
    "indentWidth": 2
  },
  "markdown": {
  },
  "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,md}"],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.57.0.wasm",
    "https://plugins.dprint.dev/json-0.13.0.wasm",
    "https://plugins.dprint.dev/markdown-0.10.0.wasm"
  ]
}
